  "license": "MIT",
  "homepage": "https://github.com/ngx-rocket/core",
  "bugs": {
    "url": "https://github.com/ngx-rocket/core/issues"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "lodash": "^4.13.1",
    "node-dir": "^0.1.14",
    "yeoman-generator": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^3.14.1"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
